package calculadora;

import static org.junit.Assert.assertArrayEquals;
import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertFalse;
import static org.junit.Assert.assertNotNull;
import static org.junit.Assert.assertNotSame;
import static org.junit.Assert.assertNull;
import static org.junit.Assert.assertSame;

import org.junit.Test;

public class AssertTest {

	@Test
	public void testAssertArrayEquals() {
		//compara arrays
		byte[] esperado = "teste".getBytes();
		byte[] atual = "teste".getBytes();
		assertArrayEquals(esperado, atual);
	}
	
	//validar strings
	@Test
	public void testAssertEquals() {
		
		assertEquals("text", "text");
	}
	
	//validar booleano 
	public void testAssertFalse() {
		assertFalse(false);
	}
	
	
	//Validar assert notNull "se objetos est達o null"
	@Test
	public void testAssertNotNull() {
		assertNotNull(new Object());
		
	}
	
	//Validar assert notSame "se objetos n達o s達o os mesmos"
		@Test
		public void testAssertNotSame() {
			assertNotSame(new Object(), new Object());
			
		}
	
		//Validar assert notNull "se objetos realmente esta null"
		@Test
		public void testAssertNull() {
			assertNull(null);
			
		}
		
		//Validar assert Same "se objetos s達o os mesmos"
		@Test
		public void testAssertSame() {
			
			Integer aNumber = Integer.valueOf(765);
			assertSame(aNumber, aNumber);
			
		}
	
}
